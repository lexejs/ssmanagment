        <tr>
            <td height="100%" width="20%">
                <asp:ContentPlaceHolder ID="cphStandartCategories" runat="server">
                </asp:ContentPlaceHolder>
            </td>
            <td height="100%" width="55%">
                <asp:ContentPlaceHolder ID="cphStandartProducts" runat="server">
                </asp:ContentPlaceHolder>
            </td>
            <td height="100%" width="25%">
                <asp:ContentPlaceHolder ID="cphStandartBuy" runat="server">
                </asp:ContentPlaceHolder>
            </td>
        </tr>
    </table>
</asp:Content>
<asp:Content ID="cntFooter" ContentPlaceHolderID="cphStandartFooter" runat="server">
</asp:Content>
