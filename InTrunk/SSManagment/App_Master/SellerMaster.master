<asp:Content ID="cntHeader" ContentPlaceHolderID="cphStandartHeader" runat="server">
    <table cellpadding="0" cellspacing="0" width="100%" 
        style="border-bottom-style: groove; border-bottom-width: 2px; margin-bottom: 8px;">
        <tr>
            <td align="right">
                <asp:LinkButton ID="lnkLogOut" runat="server">Выйти</asp:LinkButton>
            </td>
        </tr>
    </table>
</asp:Content>
